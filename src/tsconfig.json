{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [ "es2015" ],
    "declaration": true,
    "sourceMap": false,
    "noEmitHelpers": false,
    "importHelpers": true,
    "experimentalDecorators": false,
    "removeComments": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "dist",
    "node_modules",
    "spec",
    "typings"
  ]
}
